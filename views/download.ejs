    <title>Windows download - Spotify</title>
    <link rel="icon" href="//www.scdn.co/i/_global/favicon.png" data-next-head="">
    <style>
      @import url("https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap");

      * {
        margin: 0;
        padding: 0;
      }
      body {
        font-family: "Poppins", sans-serif;
        overflow: scroll;
        overflow-x: hidden;
      }
      body ::-webkit-scrollbar{
       display: none;
      }
      nav {
        height: 72px;
        width: 100%;
        background-color: black;
        display: flex;
        align-items: center;
        justify-content: space-around;
      }
      .logo svg {
        height: 100px;
        width: 118px;
        cursor: pointer;
        fill: white;
        transition: all 0.3s ease;
      }
      

      .logo svg:hover{
        transform: scale(1.04);
      }
      .logo svg:active{
        transform: scale(0.9);
      }
      .link {
        display: flex;
        align-items: center;
      }
      .link h3 {
        color: white;
        padding: 0 20px;
      }
      .link p {
        padding: 20px;
        color: white;
        font-weight: bold;
        cursor: pointer;
        font-size: 15px;
        transition: all 0.2s ease;
      }
      .link p:hover {
        transform: scale(1.04);
        color: rgb(0, 194, 0);
      }
      .link p:active{
        transform: scale(0.9);
      }
      .img-container {
        display: flex;
        align-items: center;
        position: relative;
        height: 530px;
        width: 100%;
      }
      .img-container img {
        height: 530px;
        width: 100%;
      }
      .img-content {
        position: absolute;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        color: white;
        gap: 20px;
        bottom: 118px;
        left: 28%;
      }
      .img-content h1 {
        font-size: 41px;
        margin-right: 85px;
      }
      .img-content h2 {
        font-size: 31px;
      }
      .img-content a {
        color: white;
        margin-right: 85px;
      }
      .download {
        height: 41vh;
        width: 100%;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        gap: 25px;
        background-color: rgb(255, 255, 255);
      }
      .download-links {
        display: flex;
        gap: 15px;
      }
      .download-links > img {
        border-radius: 5px;
        cursor: pointer;
        transition: all 0.3s ease;
      }
      .download-links > img:hover {
        transform: scale(1.04);
      }
      .download-links>img:active{
        transform: scale(0.9);
      }
      .cartoon {
        height: 58vh;
        position: relative;
        width: 100%;
        background-color: aquamarine;
      }
      .cartoon > img {
        height: 58vh;
        width: 100%;
      }
      .head1 {
        font-size: 14px;
        display: flex;
        gap: 20px;
        font-weight: bold;
        color: white;
      }
      .head1 a {
        color: white;
        text-decoration: none;
      }
      .head1 a:hover {
        color: rgb(0, 194, 0);
      }
      .head {
        color: white;
        width: 100%;
        height: 100px;
        gap: 10px;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        position: absolute;
        bottom: 30px;
      }
      .footer {
        display: flex;
       flex-direction: column;
        justify-content: center;
        width: 100%;
        align-items: center;
        height: 74vh;
        background-color: black;
      }
      .install-footer1 {
        padding: 50px 20px;
        display: flex;
        justify-content: space-evenly;
        width: 88%;
        
        gap: 130px;
      }
      .footer-link1{
        display: flex;
        align-items: center;
        justify-content: center;
      }
      .install-footer1 > .company,
      .communities,
      .useful-links,
      .spotify-plans {
        display: flex;
        gap: 15px;
        flex-direction: column;
      }
      .img2>svg{
        width: 135px;
        fill: white;
      }
      .install-footer1 a{
        font-size: 15px;
        font-weight: bold;
        text-decoration: none;
        color: white;
        transition: all 0.3s ease;
      }
      .install-footer1 a:hover{
        color:  rgb(0, 194, 0);
      }
      .install-footer1 h3{
        color: gray;
        font-size: 14px;
      }
      .insta{
        display: flex;
        align-items: flex-start;
        gap: 20px;
      }
      .insta>svg{
        padding: 10px;
        border-radius: 50%;
        background-color: #242323;
        cursor: pointer;
        width: 30px;
        fill: white;
        transition: all 0.3s ease;
      }
      .insta>svg:hover{
        transform: scale(1.04);
        background-color: #3f3f3f;
      }
      .insta>svg:active{
        transform: scale(0.9);
      }
      .footer-link2{
        display: flex;
        align-items: center;
        justify-content: space-between;
        width: 88%;
      }
      .footer-link2 a{
        color: gray;
        text-decoration: none;
        font-size: 11px;
        gap: 20px;
      }
      .footer-link2 a:hover{
        color: rgb(0, 194, 0);
      }
      .left{
        display: flex;
        gap: 20px;
      }
      .right{
        display: flex;
        flex-direction: column;
        gap: 10px;
      }
      .right p{
        color: gray;
        font-size: 12px;
      }
      
    </style>
